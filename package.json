{
    "name": "@wongcoin/swap-sdk",
    "version": "1.0.5",
    "description": "SDK for integrating swap functionality into your application",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./react": {
            "types": "./dist/react.d.ts",
            "import": "./dist/react.mjs",
            "require": "./dist/react.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src --ext .ts,.tsx",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "ethereum",
        "web3",
        "swap",
        "sdk"
    ],
    "author": "WongCoin",
    "license": "MIT",
    "dependencies": {
        "ethers": "^6.11.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.67",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "eslint": "^8.57.0",
        "rimraf": "^5.0.5",
        "tsup": "^8.0.2",
        "typescript": "^5.4.2"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    }
}